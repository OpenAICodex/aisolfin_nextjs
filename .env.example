# Public Supabase credentials for the client. These are safe to expose in
# browser code. Obtain them from the Supabase project settings.
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Service role key used by server routes for privileged operations such as
# writing embeddings. Keep this secret and never expose it to the client.
SUPABASE_SERVICE_ROLE_KEY=

# OpenAI credentials. The chat model should be set to a lowâ€‘cost model such as
# gpt-4.1-mini. The embedding model defaults to text-embedding-3-small.
OPENAI_API_KEY=
OPENAI_CHAT_MODEL=gpt-4.1-mini
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
# If you are using a proxy or gateway to call OpenAI, specify its base URL.
OPENAI_BASE_URL=

# Admin password used to guard the admin interface when no user role is
# available. Keep this secret and store it only on the server.
ADMIN_PASSWORD=

# Gmail configuration for the contact form. OAuth2 credentials are required
# for Nodemailer. For details see the README.
GMAIL_TO=
GMAIL_CLIENT_ID=
GMAIL_CLIENT_SECRET=
GMAIL_REFRESH_TOKEN=